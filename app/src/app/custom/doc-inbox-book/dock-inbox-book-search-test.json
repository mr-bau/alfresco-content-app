{
  "id": "mrbau.extension.search.docinboxbook",
  "order": 1,
  "default": true,
  "name": "Posteingangsbuch",

  "filterWithContains": true,
  "aca:fields": ["cm:name", "cm:title", "cm:description", "TEXT", "TAG"],
  "include": ["path", "allowableOperations", "properties", "aspectNames"],
  "sorting": {
    "options": [
        { "key": "name", "label": "Name", "type": "FIELD", "field": "cm:name", "ascending": true },
          { "key": "description", "label": "Description", "type": "FIELD", "field": "cm:description", "ascending": true }
    ],
    "defaults": [
        { "key": "name", "type": "FIELD", "field": "cm:name", "ascending": true }
    ]
  },
  "categories": [
    {
        "id": "createdDateTimeRangexxx",
        "name": "Created Datetime xxx (range)",
        "enabled": true,
        "component": {
            "selector": "datetime-range",
            "settings": {
                "field": "cm:created",
                "datetimeFormat": "DD-MM-YYYY",
                "maxDatetime": "10-Mar-20 20:00"
            }
        }
    }
  ]
}
